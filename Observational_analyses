t.mpv.hp.omp <- na.omit(t.mpv.hp.omp)
t.mpv.hp.urea <- na.omit(t.mpv.hp.urea)
t.mpv.hp.vaca <- na.omit(t.mpv.hp.vaca)

#Perform linear regression of platelet phenotypes on common infection
#PLT on TG SAG1
obs.model.PLT.TG.SAG1 <- lm(PLT_initial ~ TG_SAG1_SD + eversmoked_initialassessment + BMI_initialassessment + HDL_initialassessment + LDL_initialassessment + triglycerides_initialassessment + sex + age_initialrecruitment, data = t.plt.tg.sag1)


summary(obs.model.PLT.TG.SAG1)
summary(obs.model.PLT.TG.SAG1)$coef[2] #Observational estimate
summary(obs.model.PLT.TG.SAG1)$coef[2,2] #Standard error of observational estimate


#PLT on CMV pp52
obs.model.PLT.CMV.pp52 <- lm(PLT_initial ~ CMV_pp52_SD + eversmoked_initialassessment + BMI_initialassessment + HDL_initialassessment + LDL_initialassessment + triglycerides_initialassessment + sex + age_initialrecruitment, data = t.plt.cmv.pp52)


summary(obs.model.PLT.CMV.pp52)
summary(obs.model.PLT.CMV.pp52)$coef[2] #Observational estimate
summary(obs.model.PLT.CMV.pp52)$coef[2,2] #Standard error of observational estimate


#PLT on EBV EA-D 
obs.model.PLT.EBV.EAD <- lm(PLT_initial ~ EBV_EAD_SD + eversmoked_initialassessment + BMI_initialassessment + HDL_initialassessment + LDL_initialassessment + triglycerides_initialassessment + sex + age_initialrecruitment, data = t.plt.ebv.ead)


summary(obs.model.PLT.EBV.EAD)
summary(obs.model.PLT.EBV.EAD)$coef[2] #Observational estimate
summary(obs.model.PLT.EBV.EAD)$coef[2,2] #Standard error of observational estimate


