obs.model.MPV.HP.VacA <- lm(MPV_initial ~ HP_VacA_SD + eversmoked_initialassessment + BMI_initialassessment + HDL_initialassessment + LDL_initialassessment + triglycerides_initialassessment + sex + age_initialrecruitment, data = t.mpv.hp.vaca)

summary(obs.model.MPV.HP.VacA)
summary(obs.model.MPV.HP.VacA)$coef[2] #Observational estimate
summary(obs.model.MPV.HP.VacA)$coef[2,2] #Standard error of observational estimate

